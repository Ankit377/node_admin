<%- contentFor('HeaderCss') %> <%- contentFor('title') %> Form Elements <%-
contentFor('breadcrumb') %>
<link
href="public/libs/sweetalert2/sweetalert2.min.css"
rel="stylesheet"
type="text/css"
/>
<div class="col-sm-6">
  <div class="page-title-box">
    <h4 class="font-size-18">Add Products</h4>
    <ol class="breadcrumb mb-0">
      <li class="breadcrumb-item">
        <a href="javascript: void(0);">Veltrix</a>
      </li>
      <li class="breadcrumb-item"><a href="javascript: void(0);">Forms</a></li>
      <li class="breadcrumb-item active"></li>
    </ol>
  </div>
</div>

<%- contentFor('body') %>
<div class="row">
  <div class="col-12">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Add Time Solts</h4>
        <hr>
        <div class="form-group row">
          <label for="title" class="col-sm-2 col-form-label"
            >Title</label
          >
          <div class="col-sm-10">
            <input
              class="form-control"
              type="text"
              placeholder="Title"
              id="title"
            />
          </div>
        </div>
        <div class="form-group row">
            <label for="from" class="col-sm-2 col-form-label"
              >From Time</label
            >
            <div class="col-sm-10">
              <input
                class="form-control"
                type="text"
                placeholder=""
                id="from"
              />
            </div>
          </div>
          <div class="form-group row">
            <label for="to" class="col-sm-2 col-form-label"
              >To Time</label
            >
            <div class="col-sm-10">
              <input
                class="form-control"
                type="text"
                placeholder=""
                id="to"
              />
            </div>
          </div>
          <div class="form-group row">
            <label for="last-order-time" class="col-sm-2 col-form-label"
              >Last Order Time</label
            >
            <div class="col-sm-10">
              <input
                class="form-control"
                type="text"
                placeholder=""
                id="last-order-time"
              />
            </div>
          </div>
          <div class="form-group row">
            <label for="status" class="col-sm-2 col-form-label"
              >Status</label
            >
            <div class="col-sm-10">
              <select class="form-control">
                  <option>Active</option>
                  <option>Deactive</option>
              </select>
            </div>
          </div>
          <form action="/add-time-slots" method="POST">
            <button
                type="submit"
                class="btn btn-primary waves-effect waves-light"
               id="add-time-btn">
               Add
            </button>
        </form>
        

      </div>
    </div>
  </div>
  <!-- end col -->
</div>
<!-- end row -->

<%- contentFor('FooterJs') %> <%- contentFor('BottomJs') %>

<!-- Sweet Alerts js -->
<script src="public/libs/sweetalert2/sweetalert2.min.js"></script>

<!-- Sweet alert init js-->
<script src="public/js/pages/sweet-alerts.init.js"></script>
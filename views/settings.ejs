<%- contentFor('HeaderCss') %> <%- contentFor('title') %> Form Elements <%-
contentFor('breadcrumb') %>
<div class="col-sm-6">
    <div class="page-title-box">
        <h4 class="font-size-18">Add Products</h4>
        <ol class="breadcrumb mb-0">
            <li class="breadcrumb-item">
                <a href="javascript: void(0);">Veltrix</a>
            </li>
            <li class="breadcrumb-item"><a href="javascript: void(0);">Forms</a></li>
            <li class="breadcrumb-item active"></li>
        </ol>
    </div>
</div>

<%- contentFor('body') %>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <h4>Store Settings</h4>
                <hr>
                <% if (data != null) { %>
                <% data.forEach(result => { %>
                <form action="/update-settings" method="POST">
                    <div class="form-group row">
                        <label for="appname" class="col-sm-2 col-form-label">App Name:</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.app_name %> name="appname" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="supportnumber" class="col-sm-2 col-form-label">Support Number:</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.support_number %>
                                name="supportnumber" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="supportemail" class="col-sm-2 col-form-label">Support Email:</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.support_email %>
                                name="supportemail" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="logo" class="col-sm-2 col-form-label">Logo:</label>
                        <div class="col-sm-10">
                            <input type="file" class="filestyle" data-input="false" />
                        </div>
                    </div>
                    <h4>Version Settings</h4>
                    <hr>
                    <div class="form-group row">
                        <label for="currentversion" class="col-sm-2 col-form-label">Current Version of App:</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.current_version %>
                                name="currentversion" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="minversion" class="col-sm-2 col-form-label">Minimum Version Required:</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.min_version %> name="minversion" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="versionsystemstatus" class="col-sm-2 col-form-label"> Version System Status</label>
                        <div class="col-sm-10">
                            <input type="checkbox" id="versionStatus" switch="success" />
                            <label for="versionStatus" data-on-label="Yes" data-off-label="No"
                                name="version_value"></label>
                        </div>
                    </div>
                    <hr>
                    <div class="form-group row">
                        <label for="currency" class="col-sm-2 col-form-label">Store Currency ( Symbol or Code - $ or USD
                            -
                            Anyone ):</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value="₹" name="currency" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="gst" class="col-sm-2 col-form-label">GST ( in percentage % ):</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.gst %> name="gst" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="cgst" class="col-sm-2 col-form-label">CGST ( in percentage % ):</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.cgst %> name="cgst" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="sgst" class="col-sm-2 col-form-label">SGST ( in percentage % ):</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.sgst %> name="sgst" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="igst" class="col-sm-2 col-form-label">IGST ( in percentage % ):</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.igst %> name="igst" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="deliverychargeamt" class="col-sm-2 col-form-label">Delivery Charge Amount
                            (₹)</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value="30" name="devliverychargeamt" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="minfreedelivery" class="col-sm-2 col-form-label">Minimum Amount for Free Delivery
                            (₹)
                            ( Below this user will be charged based on Delivery Charge)</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.min_free_delivery_amount %>
                                name="minfreedelivery" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="timezone" class="col-sm-2 col-form-label">System Time Zone</label>
                        <div class="col-sm-10">
                            <select class="form-control" name="timezone">
                                <option selected><%= result.system_time_zone %></option>
                                <option>Zone 2</option>
                                <option>Zone 3</option>
                            </select>
                        </div>
                    </div>
                    <h4>Refer & Earn System</h4>
                    <hr>
                    <div class="form-group row">
                        <label for="refer" class="col-sm-2 col-form-label"> Refer and Earn System </label>
                        <div class="col-sm-10">
                            <input type="checkbox" id="refer" switch="success" checked />
                            <label for="switch4" data-on-label="Yes" data-off-label="No"></label>

                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="minreferamt" class="col-sm-2 col-form-label">Minimum Refer & Earn Order Amount
                            (₹)</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.min_refer_and_earn_amount %>
                                name="minreferamt" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="referbonus" class="col-sm-2 col-form-label">Refer & Earn Bonus (₹ OR %)</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.refer_and_earn_bonus %>
                                name="referbonus" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="refermethod" class="col-sm-2 col-form-label">Refer & Earn Method</label>
                        <div class="col-sm-10">
                            <select class="form-control" name="refermethod">
                                <option selected><%= result.refer_and_earn_method %></option>
                                <option>
                                    <%= result.refer_and_earn_method.localeCompare("Rupees") == 0 ? "Percentage" : "Rupees" %>
                                </option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="maxreferamt" class="col-sm-2 col-form-label">Maximum Refer & Earn Amount (₹)</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.max_refer_and_earn_amount %>
                                name="maxreferamt" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="minwithdrawamt" class="col-sm-2 col-form-label">Minimum Withdrawal Amount</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.min_withdraw_amount %>
                                name="minwithdrawamt" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="maxdaysreturn" class="col-sm-2 col-form-label">Max days to return item</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value="1500" name="maxdaysreturn" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="deliveryboybonus" class="col-sm-2 col-form-label">Delivery Boy Bonus (%)</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.delivery_boy_bonus %>
                                name="deliveryboybonus" />
                        </div>
                    </div>
                    <h4>Mail Settings</h4>
                    <hr>
                    <div class="form-group row">
                        <label for="fromemail" class="col-sm-2 col-form-label">From eMail ID: ( This email ID will be
                            used
                            in Mail System )</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.from_email %> name="fromemail" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="replyemail" class="col-sm-2 col-form-label">Reply To eMail ID: ( This email ID will
                            be
                            used in Mail System )</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" value=<%= result.reply_to_email %>
                                name="replyemail" />
                        </div>
                    </div>
                    <hr>
                    <button type="submit" class="btn btn-primary waves-effect waves-light" id="sa-success">
                        Update
                    </button>
                </form>
                <% }); %>
                <% } %>

            </div>
        </div>
    </div>
    <!-- end col -->
</div>
<!-- end row -->

<%- contentFor('FooterJs') %> <%- contentFor('BottomJs') %>

<script src="public/libs/sweetalert2/sweetalert2.min.js"></script>

<script src="public/js/pages/sweet-alerts.init.js"></script>

<script src="public/libs/admin-resources/bootstrap-filestyle/bootstrap-filestyle.min.js"></script>